#include<iostream>
using namespace std;
class student
{
	private:
		string name;
		int roll;
    public:
    	student(string n,int r)
    	{
    		name=n;
    		roll=r;
    		cout<<"constrector called for"<<name<<endl;
		}
		void display()
		{
			cout<<"name:"<<name<<",roll:"<<roll<<endl;
		}
	    ~student()	
		{
			cout<<"Destructer called for"<<name<<endl;
		}
};
int main()
{
	student s1("Alice",101);
	student s2("Bob",102);
	s1.display();
	s2.display();
	return 0;
}
