#include<iostream>
using namespace std;
int main()
{
	int a,b,ch;
	cout<<"enter a and b values:";
	cin>>a>>b;
	cout<<"enter operation:";
	cin>>ch;
	switch(ch)
	{
		case '+':cout<<"a+b= "<<a+b<<endl;
		         break;
		case '-':cout<<"a+b= "<<a+b<<endl;
		         break;
		case '*':cout<<"a+b= "<<a+b<<endl;
		         break;
		case '/':cout<<"a+b= "<<a+b<<endl;
		         break;
		case '%':cout<<"a+b= "<<a+b<<endl;
		         break;
		default:cout<<"invalid operation";
	}
	return 0;
}
