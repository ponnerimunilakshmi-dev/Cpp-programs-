#include<stdio.h>
#include<stdlib.h>
struct node
{
	int data;
	struct node*next;
};
typedef struct node NODE;
NODE*create();
void insertAtbegin();
void traverse();
int size=0;
NODE*head=NULL;
void create()
{
	NODE*new node=(NODE)malloc(sizeof(NODE));
	printf("enter data");
	scanf("%d",new node->data);
	size++;
	return newnode;
}
void insert at begin()
{
	NODE*newnode=create()
	if(head==NULL)
	{
		head= newnode;
	}
	else
	{
		newnode->head=NULL;
		head= newnode;
	}
	viod traverse()
	{
		NODE*tempif(head=NULL)
		printf("list is empty!!!\n");
		else
		{
			temp=head;
			while(temp!=NULL)
			{
				printf("->%d",temp->data);
				temp=temp->data;
			}
			printf("\n");
		}
		int main()
		{
			int ch;
			do
			{
				printf("1.insert at begin2.traverse");
				printf("enter your choice:");
				scanf("%d",ch)
				switch ch;
				{
					case 1:insert at begin();
				        	break;
				     case 2:traverse()
					        break;
					case 3: printf("no.of elements are%d\n",size);
					        break;
					default:exit(0);   	
				}
			}while(1);return 0;
		}
	}
	
}
