//sum of array elements
#include<iostream>
using namespace std;
int main()
{
	int a[100],n,i,sum=0;
	cout<<"Enter no of elements:";
	cin>>n;
	cout<<"Enter array elements:";
	for(i=0;i<n;i++)
	{
		cin>>a[i];
	}
	for(i=0;i<n;i++)
	{
		sum=sum+a[i];
	}
	cout<<"Sum of array elements is "<<sum<<endl;
	return 0;
}
